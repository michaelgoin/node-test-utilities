{
  "name": "@newrelic/test-utilities",
  "version": "5.1.0",
  "description": "Test library for New Relic instrumentation modules.",
  "main": "index.js",
  "scripts": {
    "docs": "git rm -r docs && jsdoc -c ./jsdoc-conf.json -d docs -r *.js lib/ && git add docs",
    "publish-docs": "./bin/publish-docs.sh",
    "lint": "eslint *.js lib/*",
    "test": "npm run unit",
    "unit": "tap tests/unit/*.tap.js tests/unit/*/*.tap.js"
  },
  "bin": {
    "versioned-tests": "./bin/version-manager.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:newrelic/node-test-utilities"
  },
  "author": "New Relic Node.js agent team <nodejs@newrelic.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0"
  },
  "devDependencies": {
    "eslint": "^4.18.0",
    "husky": "^6.0.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^11.0.0",
    "newrelic": ">=6.11.0",
    "sinon": "^3.3.0",
    "tap": "^10.7.3"
  },
  "dependencies": {
    "async": "^2.6.0",
    "colors": "^1.1.2",
    "commander": "^2.14.1",
    "concat-stream": "^1.6.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.5",
    "log-update": "^1.0.2",
    "semver": "^5.5.0"
  }
}
